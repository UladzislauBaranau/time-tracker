{
  "name": "CurrentURL",
  "description": "Sends a current tab url to your server.",
  "version": "1.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
